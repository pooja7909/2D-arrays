laptops = [[15,34,23,12],[12,43,45,23],[13,33,43,23],[45,23,32,44],[34,32,12,23],[32,33,12,23],[22,23,33,23],[12,23,12,23],[12,34,35,23],[18,23,40,33],[20,30,42,35]]
t=[]


def sum2(laptops):
    for row in range (0,len(laptops)):
        t.append(laptops[row][1])
    return t

        

total=sum2(laptops)
print(total)

